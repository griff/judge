/SpaceChar ( ) 0 get def

/trimhead { % string => string
  0 1 index length {
    2 copy get SpaceChar ne { exit } if 1 add
  } repeat 1 index length 1 index sub getinterval
} bd

/append { % string string => string
  1 index length 1 index length add string
  dup 3 index length 4 3 roll putinterval
  dup 0 4 3 roll putinterval 
} bd

/prepend { % string string => string
  1 index length 1 index length add string
  dup 2 index length 5 4 roll putinterval
  dup 0 4 3 roll putinterval 
} bd

% Split a string which is too long into 2 parts on a space character.
% Copy the empty space (the space characters at the start of the string) to the start of the second string.
% If asked, insert an extra indentation (can be everything).
% This splitting routine works well for monospaced fonts. For proportional fonts, 
% it becomes slightly more complex.
/SplitString { % string nrChars indentString => string false || string1 string2 true
  7 dict begin
  /indentString xd
  /nrChars xd
  /objectString xd
  /objectLast objectString length 1 sub def
  % Short string?
  objectLast nrChars lt {
    objectString false
  }{
    % Look for a suitable place to split.
    % First tracking backwards, if that doesn't work, look forward.
    /splitIndex -1 def
    nrChars -1 0 {
      objectString 1 index get SpaceChar eq {
        /splitIndex xd exit
      } if pop
    } for splitIndex -1 eq {
      nrChars 1 add 1 objectLast {
        objectString 1 index get SpaceChar eq {
          /splitIndex xd exit
        } if pop
      } for
    } if
    splitIndex -1 eq splitIndex objectLast eq or {
      objectString false
    }{
      % The first part.
      objectString 0 splitIndex getinterval
      % The second part.
      objectString splitIndex 1 add objectLast splitIndex sub getinterval
      % Add indentation if needed.
      indentString length 0 gt {
        dup length indentString length add string
        dup 0 indentString putinterval
        dup indentString length 4 3 roll putinterval
      } if
      % Add white space of the first part.
      /nonSpaceIndex objectLast 1 add def
      0 1 objectLast {
        objectString 1 index get SpaceChar ne {
          /nonSpaceIndex xd exit
        } if pop
      } for
      nonSpaceIndex 0 gt nonSpaceIndex splitIndex lt and {
        dup length nonSpaceIndex add string
        dup 0 objectString 0 nonSpaceIndex getinterval putinterval
        dup nonSpaceIndex 4 3 roll putinterval
      } if
      true
    } ifelse
  } ifelse
  end
} bd 

% Identify the power name at the start of the line and replace it with its shorthand version.
% The power name runs from the start of the string up to the first space or the first open bracket '(',
% or to the end of the string. In the first two cases, replace the spaces after the name with one space.
/ReplacePowerName { % string => string
  dup length 0 gt {
    dup 0 get SpaceChar ne {
      token pop load exec Shorthand
      exch trimhead append
    } if
  } if
} bd

/ExtendPowerName { % string => string
  dup length 0 gt {
    dup 0 get SpaceChar ne {
      token pop load exec Extended prepend
    } if
  } if
} bd

/WriteLine { % string => - 
  WritingCount WritingLast ge {
    % We reached the last available cell.
    /WritingCount WritingCount 1 add def
    /WritingLastString xd
  }{
    dup length WritingCount {
      % String straddles into the next column. Adapt the straddle list.
      exch WritingCoords /ColumnLength get 1 add sub dup 0 lt { pop pop exit } if
      exch WritingCoords /NrOfRows get add dup WritingLast ge {
        WritingLast eq {
          % Straddle into the last available cell. Find out which one becomes the last one now.
          WritingLast 1 sub [ WritingStraddleList aload length dup 3 add -1 roll exch { 
            2 copy lt { exit } if exch pop 1 sub 
          } repeat /WritingLast xd ] /WritingStraddleList xd
        } if pop exit 
      } if
      [ 1 index WritingStraddleList { 2 copy gt { exch } if } forall ] /WritingStraddleList xd
    } loop
    WritingCount WritingLast ge {
      % Apparently all remaining cells are already straddled. Which makes this the last cell.
      /WritingCount WritingCount 1 add def
      /WritingLastString xd
    }{
      % Great. We can write into this cell.
      currentpoint 3 2 roll show
      % Advance to the next free cell.
      WritingCount 1 add [ 4 1 roll WritingStraddleList
      WritingCoords begin
      {
        2 copy eq { 
          pop
          dup 4 1 roll NrOfRows mod 0 eq {
            pop ColumnWidth add StartY
          }{
            FontSize sub
          } ifelse 3 2 roll 1 add
        }{ 
          4 1 roll 
        } ifelse 
      } forall
      dup 4 1 roll NrOfRows mod 0 eq {
        pop ColumnWidth add StartY
      }{
        FontSize sub
      } ifelse moveto
      end
      /WritingCount xd ] /WritingStraddleList xd
    } ifelse
  } ifelse
} bd

/Write { % string => -
  ( ) {
    WritingCoords /ColumnLength get 1 add WritingCount WritingCoords /NrOfRows get add {
      dup WritingLast gt { exit } if
      exch WritingCoords /ColumnLength get 1 add add exch WritingCoords /NrOfRows get add
    } loop pop exch SplitString {
      exch WriteLine ()
    }{
      WriteLine exit
    } ifelse
  } loop
} bd

/WriteOrder { ExtendPowerName Write } bd
/WriteRetreat { ReplacePowerName Write } bd
/WriteOwner { ReplacePowerName Write } bd
/WriteAdjust { ReplacePowerName Write } bd

